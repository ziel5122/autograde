{
  "name": "autograde",
  "version": "1.1.0",
  "description": "automated grading for CSUMB Computer Science",
  "scripts": {
    "build": "NODE_ENV=production webpack -p --display-error-details",
    "build:dev": "webpack -d --display-error-details",
    "build:dev:watch": "webpack -d --watch --display-error-details",
    "start": "npm run build && babel-node src/server.js",
    "start:dev": "export NODE_ENV=development && npm run build:dev && nodemon --exec babel-node -- src/server.js",
    "start:universal": "export UNIVERSAL=true && npm run start",
    "start:dev:universal": "export NODE_ENV=development && export UNIVERSAL=true && npm run start:dev",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ziel5122/autograde.git"
  },
  "author": "Austin Zielinski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ziel5122/autograde/issues"
  },
  "homepage": "https://github.com/ziel5122/autograde#readme",
  "dependencies": {
    "aws-sdk": "^2.36.0",
    "ejs": "^2.5.6",
    "express": "^4.15.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "case-sensitive-paths-webpack-plugin": "^2.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "html-webpack-plugin": "^2.28.0",
    "postcss-loader": "^1.3.3",
    "react-dev-utils": "^0.5.2",
    "webpack": "^2.3.3",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0"
  }
}
